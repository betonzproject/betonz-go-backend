{
	"language": "postgresql",
	"useTabs": true,
	"keywordCase": "upper",
	"expressionWidth": 100,
	"paramTypes": {
		"custom": [
			{ "regex": "sqlc\\.[a-zA-Z0-9_]+\\('[a-zA-Z0-9_]+'\\)(::[a-zA-Z\"]+)+(\\[\\])" },
			{ "regex": "sqlc\\.[a-zA-Z0-9_]+\\('[a-zA-Z0-9_]+'\\)" },
			{ "regex": "@[a-zA-Z0-9_]+(::[a-zA-Z\"]+)+(\\[\\])?" },
			{ "regex": "@[a-zA-Z0-9_]+" }
		]
	}
}
